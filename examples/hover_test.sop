}

door count = 0
inta_ay (count < 5) {
    qor("Count: " + count)
    count = count + 1
    
    haddii (count == 3) {
        sii_wad
    }
    
    haddii (count == 4) {
        jooji
    }
}

// Exception handling
isku_day {
    door khalad = "No error"
    qor(khalad)
} qabo (e) {
