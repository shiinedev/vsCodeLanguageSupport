{
    "Qor (Daabac)": {
      "prefix": "q",
      "body": [
        "qor(${1:fariin})$0"
      ],
      "description": "Waxay daabacdaa fariin"
    },
  
    "Haddii (If)": {
      "prefix": "ha",
      "body": [
        "haddii (${1:shuruud}) {",
        "\t$2",
        "}"
      ],
      "description": "Haddii shuruuddu run tahay, fulin koodka"
    },
  
    "Haddii_kale (Else If)": {
      "prefix": "hk",
      "body": [
        "haddii_kale (${1:shuruud}) {",
        "\t$2",
        "}"
      ],
      "description": "Qayb kale (elif) oo shuruud ah"
    },
  
    "Haddii_kalena (Else)": {
      "prefix": "hkn",
      "body": [
        "haddii_kalena {",
        "\t$1",
        "}"
      ],
      "description": "Kale (else) marka shuruuduhu been noqdeen"
    },
  
    "Ku_Celi (Loop)": {
      "prefix": "kc",
      "body": [
        "ku_celi ${1:isbeddel} min ${2:bilow} ilaa ${3:dhamaad} {",
        "\t$4",
        "}"
      ],
      "description": "Xisaab celin (loop) min ilaa"
    },
  
    "Howl (Function)": {
      "prefix": "hl",
      "body": [
        "howl ${1:magac}(${2:qiimayaal}) {",
        "\t$3",
        "}"
      ],
      "description": "Waxay abuureysaa howl (function) cusub"
    },
  
    "Fasalka (Class)": {
      "prefix": "fl",
      "body": [
        "fasalka ${1:MagacFasal} ka_dhaxal ${2:Waalid} {",
        "\t$3",
        "}"
      ],
      "description": "Waxay abuureysaa fasal (class), ka_dhaxal (inheritance) optional"
    },
    "Inta_ay (While Loop)": {
    "prefix": "it",
    "body": [
      "inta_ay (${1:shurrud}) {",
      "\t$2",
      "}"
    ],
    "description": "Xisaab celin (loop) intaAy "
  },
  "Jooji (Break Statement)": {
    "prefix": "jj",
    "body": [
      "haddii (${1:shuruud}) {",
      "\tjooji",
      "}"
    ],
    "description": "waxa ay joojineysa(break) shuruuda"
  },
  "Sii_Wad (Continue Statement)": {
    "prefix": "sw",
    "body": [
      "haddii (${1:shuurud}) {",
      "\tsii_wad",
      "}"
    ],
    "description": "waxa ay sii wadeysa(Continue) shuruuda"
  },
  "Soo_celi (Return Statement)": {
    "prefix": "sc",
    "body": [
      "soo_celi ${1:qiimaha}$0"
    ],
    "description": "waxey so celineysa(return) qiimo"
  },
  "Isku_day (Try Block)": {
    "prefix": "ikd",
    "body": [
      "isku_day {",
      "\t$1",
      "}"
    ],
    "description": " waxey isku dayeysa(Try) qeyb(block)"
  },
  "Qabo (Catch Block)": {
    "prefix": "qb",
    "body": [
