{
  "name": "soplang",
  "displayName": "Soplang",
  "description": "VS Code syntax highlighting for Soplang",
  "version": "0.0.1",
  "publisher": "soplang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soplang/vsCodeLanguageSupport"
  },
  "bugs": {
    "url": "https://github.com/soplang/vsCodeLanguageSupport/issues"
  },
  "homepage": "https://github.com/soplang/vsCodeLanguageSupport",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "images/soplang-icon.png",
  "contributes": {
    "languages": [
      {
        "id": "soplang",
        "aliases": [
          "Soplang",
          "soplang"
        ],
        "extensions": [
          ".so",
          ".sop"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "soplang",
        "scopeName": "source.soplang",
        "path": "./syntaxes/soplang.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "soplang",
        "path": "./snippets/soplang.json"
      }
    ],
    "iconThemes": [
      {
        "id": "soplang-file-icon",
        "label": "Soplang File Icon",
        "path": "./file-icons/soplang-icon-theme.json"
      }
    ],
    "configurationDefaults": {
      "workbench.iconTheme": "soplang-file-icon",
      "cSpell.enableFiletypes": [
        "soplang"
      ],
      "cSpell.enabled": true,
      "cSpell.languageSettings": [
        {
          "languageId": "soplang",
          "dictionaries": [
            "soplang"
          ]
        }
      ]
    },
    "cSpell.dictionaries": [
      {
        "name": "soplang",
        "description": "Soplang programming language keywords and syntax terms.",
        "path": "./dictionaries/soplang.txt"
      }
    ]
  },
  "extensionPack": [
    "streetsidesoftware.code-spell-checker"
  ],
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "vsce": "^2.12.0",
    "vscode": "^1.1.37"
  },
  "activationEvents": [
    "onLanguage:soplang"
  ],
  "main": "./out/extension.js",
  "files": [
    "dictionaries/**"
  ]
}